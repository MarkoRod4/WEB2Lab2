<!DOCTYPE html>
<html lang="hr">
<head>
    <meta charset="UTF-8">
    <link rel="stylesheet" href="/style.css">
    <title>Korisnici</title>
</head>
<body>
    <h1>Pregled korisnika</h1>
    <button onclick="window.location.href='/';">Back</button>

    <% if (users && users.length > 0) { %>
        <table>
            <thead>
                <tr>
                    <th>Username</th>
                    <th>Password</th>
                    <th>Hashed</th>
                </tr>
            </thead>
            <tbody>
                <% users.forEach(function(user) { %>
                    <tr>
                        <td><%= user.username %></td>
                        <td><%= user.password %></td>
                        <td><%= user.vulnerable ? 'No' : 'Yes' %></td>
                    </tr>
                <% }); %>
            </tbody>
        </table>
    <% } else { %>
        <p>Nema korisnika</p>
    <% } %>
</body>
</html>
